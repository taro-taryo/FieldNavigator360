{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Server",
        "program": "${workspaceFolder}/server/app/server.js",
        "cwd": "${workspaceFolder}",
        "runtimeArgs": ["--inspect=9229"],
        "env": {
          "NODE_ENV": "development"
        },
        "outputCapture": "std",
        "skipFiles": ["<node_internals>/**"]
      }
    ]
  }
  